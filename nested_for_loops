// syntax for nested for loop
/*
for ( init; condition; increment ) {

   for ( init; condition; increment ) {
      statement(s);
   }
   statement(s);
}
*/

#include<stdio.h>
int main()
{
    int n;
    printf("Enter the value of n :");
    scanf("%d", &n);
    // Displaying the n tables
    for(int i=1;i<=n;i++) //outer loop
    {
        for(int j=1;j<=10;j++) //inner loop
        {
            printf("%d\t",(i*j)); //printing the value
        }
        printf("\n");
    }
}